{
  "angular-schematics.schematicsDefaultOptions": {
    "angular-*": {
      "externalTemplate": true,
      "skipStyle": true
    },
    "angular-component": {
      "skipChangeDetectionOnPush": true
    },
    "angular-page": {
      "skipChangeDetectionOnPush": true
    }
  },
  "[html]": {
    "editor.defaultFormatter": "prettier.prettier-vscode"
  },
  "prettier.bracketSameLine": true
}
